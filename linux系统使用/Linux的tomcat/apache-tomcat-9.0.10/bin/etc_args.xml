    <diskSetting>
        <!--电子盘-->
        <sys_disk>/home</sys_disk>
        <!--数据盘-->
        <data_disk>/home/EMTC</data_disk>
    </diskSetting>
    <switchSetting>
        <!--授权开关 0：关闭，1：开启-->
        <authorize_switch>0</authorize_switch>
        <!--读写路径信息开关 0：关闭，1：开启-->
