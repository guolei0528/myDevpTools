        <!--2点至5点只允许客一通行 0：不开启，1：开启-->
        <passcar_setting>1</passcar_setting>
        <!--车牌识别匹配设置 1:按时间匹配，3-6:设置模糊匹配度，值越大越精确-->
        <plate_match>1</plate_match>
        <!--是否启用TSAM卡，0:未启用，1:已启用-->
        <tsam_switch>0</tsam_switch>
        <!--是否下载工号表，0:下载，1:不下载-->
        <downoperator_switch>0</downoperator_switch>
