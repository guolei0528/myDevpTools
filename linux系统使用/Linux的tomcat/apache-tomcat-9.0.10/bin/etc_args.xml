        <!--车牌不一致判断，0：不判断；1：判断-->
        <vehPlate_chk>1</vehPlate_chk>
        <!--抬栏杆授权，0：不授权；1：授权-->
        <Parapet_chk>0</Parapet_chk>
        <!--军车授权，0：不授权；1：授权-->
        <army_chk>0</army_chk>
        <!--轴重授权，0：不授权；1：授权-->
        <axis_chk>0</axis_chk>
        <!--牵引授权，0：不授权；1：授权-->
        <hauler_chk>0</hauler_chk>
        <!--无卡授权，0：不授权；1：授权-->
        <noCard_chk>0</noCard_chk>
        <!--卡损授权，0：不授权；1：授权-->
        <badCard_chk>0</badCard_chk>
        <!--优惠授权，0：不授权；1：授权-->
        <rebate_chk>0</rebate_chk>
        <!--上班授权，0：不授权；1：授权-->
        <onShift_chk>0</onShift_chk>
        <!--PSAM验证，0：不验证；1验证：-->
        <onPSAM_chk>1</onPSAM_chk>
        <!--落杆返回信号，0：没有；1：有-->
        <onFalling_back>1</onFalling_back>
