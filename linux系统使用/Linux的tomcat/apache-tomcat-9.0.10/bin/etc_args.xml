    <!--ETC天线参数-->
    <rsuSetting>
        <!--天线1等待时间-->
        <rsu1_wait_time>10</rsu1_wait_time>
        <!--天线1功率-->
        <rsu1_power>29</rsu1_power>
        <!--天线1信道-->
        <rsu1_channel>0</rsu1_channel>
        <!--天线2等待时间-->
        <rsu2_wait_time>10</rsu2_wait_time>
        <!--天线2功率-->
        <rsu2_power>30</rsu2_power>
        <!--天线2信道-->
        <rsu2_channel>1</rsu2_channel>
        <!--天线数量-->
        <rsu_cnt>1</rsu_cnt>
    </rsuSetting>
    <!--串口设置-->
    <comSetting>
        <!--天线1串口-->
        <rsu1_com>1</rsu1_com>
        <!--天线2串口-->
        <rsu2_com>2</rsu2_com>
        <!--读卡器串口-->
        <card_com>3</card_com>
        <!--MTC费显串口-->
        <mtc_display_com>4</mtc_display_com>
        <!--ETC费显串口-->
        <etc_display_com>1</etc_display_com>
        <!--计重仪串口-->
        <weight_com>4</weight_com>
        <!--打印机串口-->
        <printer_com>4</printer_com>
        <!--字符叠加串口-->
        <imp_com>2</imp_com>
        <!--省界站串口-->
        <coop_com>4</coop_com>
    </comSetting>
    <!--硬件环境参数-->
    <hardwareSetting>
        <!--LED屏IP-->
        <led_ip>192.168.4.120</led_ip>
        <!--车牌识别数量-->
        <license_cnt>1</license_cnt>
        <!--ETC模式车牌抓拍线圈控制,只能设置1和5两个值-->
        <etclicense_capCoil>5</etclicense_capCoil>
        <!--读写器厂家；0-航天/1-金溢-->
        <ic_reader>0</ic_reader>
        <!--车牌识别厂家；0-汉王(老设备)/5-汉王(新设备)/1-天华威视/2-高德威/3-上海聚德/4-沈阳具德/6-汉王(TC200)-->
        <license_type>0</license_type>
        <!--mtc费显协议类型；0-标准（52位）/1-智运专用（32位）/2-顶恩 /3-顶基-->
        <mtcdispshow_agreement_type>1</mtcdispshow_agreement_type>
        <!--称重仪通信协议类型；0-00指令 标准方式/1-07指令bb结束 智运专用-->
        <axis_agreement_type>1</axis_agreement_type>
        <!--称重仪状态获取方式类型；0-称重仪主动发送/1-系统主动问询-->
        <axis_getState_type>0</axis_getState_type>
        <!--称重仪帧回复，仅回复车辆称重信息；0-不回复/1-回复 不同厂家称重仪如果不回复车辆称重信息帧，将持续发送同一帧-->
        <axis_reply_type>0</axis_reply_type>
        <!--车检信号输入类型；0-电压/1-开关/2-全电压-->
        <coil_input_type>2</coil_input_type>
        <!--QT显示类型；0：视频/1：图片-->
        <video_type>0</video_type>
        <!--打印机接口类型；1：串口/2：并口-->
        <printer_type>2</printer_type>
        <!--打印机纸张类型:0：76.5mm纸张，1：89.5mm纸张-->
        <printer_paper>1</printer_paper>
        <!--打印机判车型跳票设置:0：跳票，1：不跳票-->
        <printer_skip>0</printer_skip>
        <!--打印机设备类型设置，0：star(sp320),1：得实打印机(sp300),2：腾浩(spr320)-->
        <printer_comp>0</printer_comp>
        <!--IO卡型号，0：725卡,1：1761卡-->
        <device_type>0</device_type>
        <!--ETC超时删除交易时间，以秒为单位-->
        <del_etcTrans>60</del_etcTrans>
        <!--昨天计算机重启次数，默认值是0-->
        <reboot_num1>0</reboot_num1>
        <!--今天计算机重启次数，默认值是0-->
        <reboot_num2>0</reboot_num2>
        <!--入口超时时间设置，单位：小时-->
        <entry_timeout>10</entry_timeout>
        <!--ETC卡现金支付后是否弹出打印选择窗:0：不弹，1：弹-->
        <printer_win>0</printer_win>
        <!--储值卡余额充足情况下是否允许现金支付:0：允许，1：不允许-->
        <cash_pay>1</cash_pay>
        <!--ETC卡现金支付后是否打印:0：默认打印，1：通过弹窗选择-->
        <ccard_print>1</ccard_print>
        <!--刷通行卡车牌识别无颜色时是否补加颜色:0：默认不补，1：补加-->
        <add_color>0</add_color>
    </hardwareSetting>
    <!--监控配置-->
    <monitorSetting>
        <!--中心监控IP-->
        <monitor_ip>192.168.4.120</monitor_ip>
        <!--中心通讯IP-->
        <center_ip>192.168.4.120</center_ip>
        <!--沈阳具德车牌识别IP-->
        <plate_ip>10.132.91.5</plate_ip>
        <!--收费站授权端IP-->
        <embranchment_ip>192.168.1.69</embranchment_ip>
        <!--授权等待时间-->
        <wait_time>60</wait_time>
