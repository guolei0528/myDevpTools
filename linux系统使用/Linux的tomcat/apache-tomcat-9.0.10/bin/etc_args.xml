<?xml version="1.0" encoding="gb2312" ?>
<etcSetting>
    <!--收费站信息-->
    <stationSetting>
        <!--所属路段-->
        <road>沿江高速</road>
        <!--网络编号；3201-南网/3202-北网-->
        <network>3201</network>
        <!--收费站编号：限7位-->
        <station>650002</station>
        <!--收费站名称-->
        <station_name>戚墅堰</station_name>
        <!--欢迎词-->
        <welcome>戚墅堰欢迎你</welcome>
        <!--车道号-->
        <lane>2</lane>
        <!--车道收费模式；1-ETC车道/2-混合车道/4-免费/5-MTC车道-->
        <stn_tollmode>5</stn_tollmode>
        <!--车道标识；3-入口车道/4-出口车道/6-开放车道/7-标识站/8-省界车道-->
        <work_mode>4</work_mode>
        <!--布局模式；1-单栏杆模式（栏杆前置，仅ETC收费模式）/2-单栏杆模式（栏杆后置，ETC和MTC模式混用）/3-双栏杆模式-->
        <layout_mode>3</layout_mode>
        <!--车道编号（传输用） -->
        <roadno>01001</roadno>
    </stationSetting>
    <!--工班信息-->
    <operatorSetting>
        <!--工班统计时间-->
        <statis_time>80</statis_time>
        <!--操作员-->
        <operator>971</operator>
    </operatorSetting>
    <!--收费站数据库信息-->
    <stndbSetting>
        <!--收费站数据库类型；0-sqlserver/1-mysql-->
        <db_stntype>0</db_stntype>
        <!--数据库IP-->
        <db_stnhost>10.132.51.1</db_stnhost>
        <!--数据库用户名-->
        <db_stnuser>sa</db_stnuser>
        <!--数据库密码-->
        <db_stnpswd>intellitrans</db_stnpswd>
        <!--数据库名称-->
        <db_stnname1>DBETC</db_stnname1>
        <!--数据库名称-->
        <db_stnname2>LICENSEIMAGE</db_stnname2>
        <!--数据库名称-->
        <db_stnname3>BASEINFO</db_stnname3>
        <!--传输标志-->
        <privateno>2</privateno>
        <!--MTC承包商标志 1:智运;2:西公所;3:长天智远-->
        <db_contractor>2</db_contractor>
        <!--键盘类型 1:KY41R  2:KY41S-->
        <keyboardtype>2</keyboardtype>
    </stndbSetting>
    <!--车道机数据库信息-->
    <lanedbSetting>
        <!--数据库IP-->
        <db_host>127.0.0.1</db_host>
        <!--数据库用户名-->
        <db_user>etc_ctrlr</db_user>
        <!--数据库密码-->
        <db_pswd>itsmoe</db_pswd>
    </lanedbSetting>
    <!--省界站信息-->
    <coopstnSetting>
